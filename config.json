{
  "log": {
    "level": "info"
  },
  "dns": {
    "servers": [
      {
        "address": "tls://1.1.1.1"
      }
    ]
  },
  "ntp": {
    "enabled": true,
    "server": "time.apple.com",
    "server_port": 123
  },
  "inbounds": [
    {
      "type": "trojan",
      "tag": "trojan",
      "listen": "127.0.0.1",
      "listen_port": 52000,
      "tcp_fast_open": true,
      "udp_fragment": true
    },
    {
      "type": "trojan",
      "tag": "trojan-ws",
      "listen": "127.0.0.1",
      "listen_port": 52001,
      "tcp_fast_open": true,
      "udp_fragment": true,
      "fallback": {
        "server": "127.0.0.1",
        "server_port": 52004
      },
      "transport": {
        "type": "ws",
        "path": "/multi"
      }
    },
    {
      "type": "trojan",
      "tag": "trojan-grpc",
      "listen": "127.0.0.1",
      "listen_port": 52002,
      "tcp_fast_open": true,
      "udp_fragment": true,
      "fallback": {
        "server": "127.0.0.1",
        "server_port": 0
      },
      "transport": {
        "type": "grpc",
        "service_name": "multi"
      }
    },
    {
      "type": "vmess",
      "tag": "vmess",
      "listen": "127.0.0.1",
      "listen_port": 52003,
      "tcp_fast_open": true,
      "udp_fragment": true
    },
    {
      "type": "vmess",
      "tag": "vmess-ws",
      "listen": "127.0.0.1",
      "listen_port": 52004,
      "tcp_fast_open": true,
      "udp_fragment": true,
      "transport": {
        "type": "ws",
        "path": "/multi"
      }
    },
    {
      "type": "vmess",
      "tag": "vmess-grpc",
      "listen": "127.0.0.1",
      "listen_port": 52005,
      "tcp_fast_open": true,
      "udp_fragment": true,
      "transport": {
        "type": "grpc",
        "service_name": "multi"
      }
    },
    {
      "type": "vless",
      "tag": "vless",
      "listen": "127.0.0.1",
      "listen_port": 52006,
      "tcp_fast_open": true,
      "udp_fragment": true
    },
    {
      "type": "vless",
      "tag": "vless-ws",
      "listen": "127.0.0.1",
      "listen_port": 52007,
      "tcp_fast_open": true,
      "udp_fragment": true,
      "transport": {
        "type": "ws",
        "path": "/vless"
      }
    },
    {
      "type": "vless",
      "tag": "vless-grpc",
      "listen": "127.0.0.1",
      "listen_port": 52008,
      "tcp_fast_open": true,
      "udp_fragment": true,
      "transport": {
        "type": "grpc",
        "service_name": "vless"
      }
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    }
  ],
  "route": {
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      {
        "ip_cidr": ["1.1.1.1", "8.8.8.8"],
        "outbound": "direct"
      },
      {
        "port": 53,
        "outbound": "direct"
      }
    ],
    "final": "direct"
  },
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "/usr/local/latinaserver/dashboard/"
    },
    "v2ray_api": {
      "listen": "127.0.0.1:5555",
      "stats": {
        "enabled": true,
        "inbounds": [
          "trojan",
          "trojan-ws",
          "trojan-grpc",
          "vmess",
          "vmess-ws",
          "vmess-grpc",
          "vless",
          "vless-ws",
          "vless-grpc"
        ],
        "outbounds": ["direct"]
      }
    }
  }
}
